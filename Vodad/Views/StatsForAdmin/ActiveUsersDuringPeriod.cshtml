<h2>Active users</h2>

<b>@ViewBag.ActiveUsersSum
&nbsp;active users</b><br/>

<input type="text" value="@DateTime.Today" class="start-date" />
<input type="text" value="@DateTime.Today" class="end-date" />
<input type="button" id="update-button" value="Update" class="button" />
<br/>

<table id="active-users-table">
    <thead>
        <tr>
            <th>Last active date</th>
            <th>Name</th>
            <th>Role</th>
            <th>Activated</th>
     </tr>
    </thead>
        <tbody>
@foreach (var c in ViewBag.ActiveUsersDuringPeriod)
{
    <tr>
                    <td>@c.LastActiveDate</td>
                    <td>@c.Name</td>
                    <td>@c.Role</td>
                    <td>@c.IsActivated</td>
                </tr>

}
            </tbody>
            </table>